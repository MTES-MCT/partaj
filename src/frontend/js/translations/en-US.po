#. [components.ReferralDetailAnswer.contentInputLabel] - Label for the content input field for a referral answer
#. defaultMessage is:
#. Add an answer for this referral
#: js/translations/sources/js/components/ReferralDetailAnswer/index.json
msgid "Add an answer for this referral"
msgstr "Add an answer for this referral"

#. [components.ReferralDetailAnswer.submitAnswer] - Button to submit the answer to a referral
#. defaultMessage is:
#. Answer the referral
#: js/translations/sources/js/components/ReferralDetailAnswer/index.json
msgid "Answer the referral"
msgstr "Answer the referral"

#. [components.ReferralDetailAssignment.assignAMember] - Button in referral detail view that allows unit organizers to assign the referral to a member
#. defaultMessage is:
#. Assign a member
#: js/translations/sources/js/components/ReferralDetailAssignment/index.json
msgid "Assign a member"
msgstr "Assign a member"

#. [components.ReferralDetailAssignment.assignedTo] - Associated text for the list of unit members a referral has been assigned to on the referral detail view.
#. defaultMessage is:
#. Assignment(s)
#: js/translations/sources/js/components/ReferralDetailAssignment/index.json
msgid "Assignment(s)"
msgstr "Assignment(s)"

#. [components.ReferralDetailAnswer.byWhom] - Author of the referral answer
#. defaultMessage is:
#. By {name}, {unit_name}
#: js/translations/sources/js/components/ReferralDetailAnswer/index.json
msgid "By {name}, {unit_name}"
msgstr "By {name}, {unit_name}"

#. [components.ReferralDetailAnswer.loadingAnswer] - Accessible loading message for the answer spinner on the referral detail view
#. defaultMessage is:
#. Loading answer...
#: js/translations/sources/js/components/ReferralDetailAnswer/index.json
msgid "Loading answer..."
msgstr "Loading answer…"

#. [components.ReferralDetailAssignment.loadingAssignees] - Accessible loading message for the assignees spinner on the referral detail view.
#. defaultMessage is:
#. Loading assignees...
#: js/translations/sources/js/components/ReferralDetailAssignment/index.json
msgid "Loading assignees..."
msgstr "Loading assignees…"

#. [components.ReferralDetailAssignment.noAssigneeYet] - Default text when there are no assignees on the referral detail view.
#. defaultMessage is:
#. No assignment yet
#: js/translations/sources/js/components/ReferralDetailAssignment/index.json
msgid "No assignment yet"
msgstr "No assignment yet"

#. [components.ReferralDetailAnswer.answer] - Title for the answer part of the referral detail view
#. defaultMessage is:
#. Referral answer
#: js/translations/sources/js/components/ReferralDetailAnswer/index.json
msgid "Referral answer"
msgstr "Referral answer"

#. [components.ReferralDetailAnswer.startWriting] - Explanation next to the disabled submit button when writing a referral answer.
#. defaultMessage is:
#. You need to start writing an answer to send it.
#: js/translations/sources/js/components/ReferralDetailAnswer/index.json
msgid "You need to start writing an answer to send it."
msgstr "You need to start writing an answer to send it."
